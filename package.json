{
  "name": "teaserpaste-cli",
  "version": "0.1.3",
  "description": "CLI Client for TeaserPaste - View and Clone snippets.",
  "main": "cli.js",
  "bin": {
    "tp": "cli.js"
  },
  "scripts": {
    "start": "node cli.js",
    "build": "pkg ."
  },
  "dependencies": {
    "node-fetch": "^2.6.7"
  },
  "devDependencies": {
    "pkg": "^5.8.1"
  },
  "preferGlobal": true,
  "pkg": {
    "targets": [
      "node18-win-x64",
      "node18-linux-x64",
      "node18-macos-x64"
    ],
    "outputPath": "dist"
  }
}
